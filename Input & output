#Hello World
print("hello world")

#Hello World with tab
print("Hello World\t hello World")

#Hello World with new line
print("Hello World\nHello World")

#Integer Input
a=int(input())
print(a)

#Float Input
a=float(input())
print("%.2f"%a)

#Character Input
a=input()
print(a)

#String Input
a=str(input())
print(a)

#Ascii-I
a=input()
print(ord(a))

#Ascii-II
a=int(input())
print(chr(a))
